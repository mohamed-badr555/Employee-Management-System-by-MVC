@model ResetPasswordViewModel
@{
    ViewData["Title"] = "Reset Password";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ResetPassword">
			<div asp-validation-summary="All"></div>
			

			<input class="text" asp-for="NewPassword" placeholder="New Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
			
				<div class="clear"> </div>
			</div>
			<input type="submit" value="ResetPassword">
		</form>

	</div>
</div>

@section ValidationScriptSection {
	<partial name="_ValidationScriptsPartial" />

}


